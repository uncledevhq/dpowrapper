{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "dpowrapper",
  "author": "uncledevhq",
  "module": "dist/dpowrapper.esm.js",
  "size-limit": [
    {
      "path": "dist/dpowrapper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dpowrapper.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/axios": "^0.9.36",
    "husky": "^9.1.6",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "fast-xml-parser": "^4.5.0"
  }
}
